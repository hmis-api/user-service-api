#%RAML 1.0
title: "HMIS User Service"
version: "1.0.0"
protocols:
- "HTTPS"
securedBy: [oauth_2_0]
securitySchemes:
  oauth_2_0: !include security-schemes/oauth_2_0.raml
baseUri: "https://www.hmislynk.com/hmis-user-service/rest"
/accounts:
  description: "All accounts"
  post:
    description: "It creates an account with the specified username and sends out\
      \ an email to the user to verify the account creation. The initial status of\
      \ the newly created account is PENDING. The status is changed to ACTIVE when\
      \ the user verifies account creation by clicking the link embedded in the email.\
      \ Only an ACTIVE account can log-in using the Create a session (Log-in) API.\
      \ <br> It returns an error if an account with the specified username already\
      \ exists.\nThis endpoint does the following:\n1) If request does not contain\
      \ profile information, standard profile will be assigned to the user.\n2) If\
      \ request does not contain role information, default role will be assigned based\
      \ on profile.    \n3) Project Group of the user will be same as the project\
      \ group of logged-in user who creates the account.\nThe user must have \"USR_CREATE_ACCOUNT\"\
      \ permission to use this API    \n"
    headers:
      Authorization:
        type: "string"
        description: "Authorization request header should specify as HMISUserAuth\
          \ session_token={sessiontokenvalue}."
        example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
      X-HMIS-TrustedApp-Id:
        type: "string"
        description: "Trusted App id should be provide in this header."
        example: "MASTER_TRUSTED_APP"
    responses:
      200:
        body:
          application/json:
            type: "object"
      409:
        description: "An account with the specified username already exists."
        body:
          application/json:
            type: "object"
  get:
    description: "To get all the users under logged in your project group."
    queryParameters:
      startIndex:
        type: "string"
        default: "0"
        required: false
      maxItems:
        type: "string"
        default: "30"
        required: false
    headers:
      Authorization:
        type: "string"
        description: "Authorization request header should specify as HMISUserAuth\
          \ session_token={sessiontokenvalue}."
        example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
      X-HMIS-TrustedApp-Id:
        type: "string"
        description: "Trusted App id should be provide in this header."
        example: "MASTER_TRUSTED_APP"
    responses:
      200:
        body:
          application/json:
            type: "object"
  /{accountid}:
    uriParameters:
      accountid:
        type: "string"
    put:
      description: "To update a specific account information.\nProvide account id as\
        \ path variable for upatading non loggedin user. If need to update logged in\
        \ user send self as account id.\n"
      responses:
        200:
          body:
            application/json:
              type: "object"
    get:
      description: "Gets (aka reads or retrieves) a specific account. <BR><br>\nGET\
        \ /accounts/{username}\n<b>\n"
      headers:
        Authorization:
          type: "string"
          description: "Authorization request header should specify as HMISUserAuth\
            \ session_token={sessiontokenvalue}."
          example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
        X-HMIS-TrustedApp-Id:
          type: "string"
          description: "Trusted App id should be provide in this header."
          example: "MASTER_TRUSTED_APP"
      responses:
        200:
          description: "Successfully retrieved resource(s)\n"
          body:
            application/json:
              type: "object"
        403:
          body:
            application/json:
              type: "object"
    delete:
      description: "Account status will be updated to DELETED. \n"
      headers:
        Authorization:
          type: "string"
          description: "Authorization request header should specify as HMISUserAuth\
            \ session_token={sessiontokenvalue}."
          example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
        X-HMIS-TrustedApp-Id:
          type: "string"
          description: "Trusted App id should be provide in this header."
          example: "MASTER_TRUSTED_APP"
    /activate:
      put:
        description: "To reactivate a specified user account\n"
        headers:
          Authorization:
            type: "string"
            description: "Authorization request header should specify as HMISUserAuth\
              \ session_token={sessiontokenvalue}."
            example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
          X-HMIS-TrustedApp-Id:
            type: "string"
            description: "Trusted App id should be provide in this header."
            example: "MASTER_TRUSTED_APP"
        responses:
          200:
            body:
              application/json:
                type: "object"
          403:
            body:
              application/json:
                type: "object"
      /basicinfo:
        description: "A specific account"
        get:
          description: "Gets (aka reads or retrieves) a specific account. <BR><br>\nGET\
