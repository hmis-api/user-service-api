              example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
            X-HMIS-TrustedApp-Id:
              type: "string"
              description: "Trusted App id should be provide in this header."
              example: "MASTER_TRUSTED_APP"
          responses:
            200:
              description: "Successfully retrieved resource(s)\n"
              body:
                application/json:
                  type: "object"
            403:
              body:
                application/json:
                  type: "object"
      /passwordchanges:
        description: "Change password for an account"
        put:
          description: "Changes password for an account. To change the password, the user\
            \ must be logged-in and provide his/her current password.\nThe {username} is\
            \ a value that uniquely identifies an account.\nTo change the password of the\
            \ currently logged-in user, the {username} can be replaced by <b>self</b> keyword\
            \ in the URL as shown below.\n<b>POST</b> /accounts/self/passwordchanges\n"
          headers:
            Authorization:
              type: "string"
              description: "Authorization request header should specify as HMISUserAuth\
                \ session_token={sessiontokenvalue}."
              example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
            X-HMIS-TrustedApp-Id:
              type: "string"
              description: "Trusted App id should be provide in this header."
              example: "MASTER_TRUSTED_APP"
          responses:
            200:
              description: ""
              body:
                application/json:
                  type: "object"
            403:
              body:
                application/json:
                  type: "object"
      /forgotpassword:
        description: "Change password for an account"
        post:
          description: "Resets password for an account. It sends out an email notification\
            \ to the user to verify the password reset. The new password becomes effective\
            \ only after it has been verified by the user. It returns an error if an account\
            \ with the specified username does not exist OR the account has been DISABLED.\n\
            </br> The {username} is a value that uniquely identifies an account.\n"
          headers:
            Authorization:
              type: "string"
              description: "Authorization request header should specify as HMISUserAuth\
                \ session_token={sessiontokenvalue}."
              example: "HMISUserAuth session_token=956048930B39448A8B6ABEDF57BECFFC07E2628546F847149BD42D4BC3CFDC39"
            X-HMIS-TrustedApp-Id:
              type: "string"
              description: "Trusted App id should be provide in this header."
              example: "MASTER_TRUSTED_APP"
          responses:
            200:
              description: ""
              body:
                application/json:
                  type: "object"
            403:
              body:
                application/json:
                  type: "object"
            404:
              body:
                application/json:
                  type: "object"
    /roles:
      put:
        description: "To assigne a role to user. If the role is already assigned nothing\
